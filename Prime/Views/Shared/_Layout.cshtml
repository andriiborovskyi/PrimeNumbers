<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="row">
        <div class="col-md-12">
            <h2>Select Approach</h2>
        </div>
    </div>
    <div class="row">
                <div class="col-md-3">
                    @Html.ActionLink( "Sqrt", "GeneretePrime", new { algorithmType = (int)Prime.Generation.PrimeGeneratorType.Sqrt, quantity = 100 } )
                </div>
                <div class="col-md-3">
                    @Html.ActionLink( "Sieve Of Eratosthenes", "GeneretePrime", new { algorithmType = (int)Prime.Generation.PrimeGeneratorType.SieveOfEratosthenes, quantity = 100 } )
                </div>
                <div class="col-md-3">
                    @Html.ActionLink( "Static", "GeneretePrime", new { algorithmType = (int)Prime.Generation.PrimeGeneratorType.Static, quantity = 100 } )
                </div>
                <div class="col-md-3">
                    @Html.ActionLink( "Parallel", "GeneratePrimeParallel", new { quantity = 100 } )
                </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
